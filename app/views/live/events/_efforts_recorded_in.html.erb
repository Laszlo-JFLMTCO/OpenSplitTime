<div class="row">
  <div class="col-xs-10">
    <h4><%= "Efforts recorded in at #{@aid_station_detail.split_name} as of #{l(Time.now, format: :full_with_weekday)} at #{l(Time.now, format: :military)}" %></h4>
    <table class="table table-condensed table-striped">
      <thead>
      <tr>
        <th>Bib</th>
        <th>Name</th>
        <th>Category</th>
        <th>From</th>
        <th class="text-center">Prior Recorded</th>
        <th class="text-center">Recorded Out Here</th>
      </tr>
      </thead>
      <tbody>
      <% @aid_station_detail.recorded_in_progress_efforts.each do |progress_effort| %>
      <% prior_valid_display_data = @aid_station_detail.prior_valid_display_data %>
          <tr>
            <td><%= progress_effort.bib_number %></td>
            <td class="text-nowrap"><strong><%= link_to progress_effort.full_name, effort_path(progress_effort.id) %></strong></td>
            <td class="text-nowrap"><%= progress_effort.bio_historic %></td>
            <td class="text-nowrap"><%= progress_effort.state_and_country %></td>
            <td class="text-center text-nowrap"><%= "#{prior_valid_display_data[:split_name]}, #{l(prior_valid_display_data[:day_and_time], format: :day_and_military)}" %></td>
            <td class="text-center text-nowrap"><%= l(@aid_station_detail.recorded_out_tfs(progress_effort), format: :day_and_military) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>