<% content_for :title do %>
    <% "OpenSplitTime: List users" %>
<% end %>
<div class="container">
  <div class="row">
    <div class="col-md-8">
      <h3>Users</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-md-8">
      <table class="table">
        <tbody>
        <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
