<table class="table table-condensed table-striped">
  <thead>
  <tr>
    <th>Bib</th>
    <th>Name</th>
    <th>Category</th>
  </tr>
  </thead>
  <tbody>
  <% @mini_table.effort_rows.each do |row| %>
      <tr>
        <td><%= row.bib_number %></td>
        <td><%= link_to row.full_name, effort_path(row.id) %></td>
        <td><%= row.bio %></td>
      </tr>
  <% end %>
  </tbody>
</table>
